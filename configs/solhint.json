{
  "rules": {
    "avoid-call-value": "warn",
    "avoid-low-level-calls": "off",
    "avoid-sha3": "warn",
    "avoid-suicide": "warn",
    "avoid-throw": "warn",
    "avoid-tx-origin": "warn",
    "check-send-result": "warn",
    "code-complexity": ["warn", 5],
    "compiler-version": ["warn", "^0.8.0"],
    "comprehensive-interface": "off",
    "const-name-snakecase": "off",
    "constructor-syntax": "warn",
    "contract-name-camelcase": "warn",
    "event-name-camelcase": "warn",
    "explicit-types": "warn",
    "foundry-test-functions": "off",
    "func-name-mixedcase": "warn",
    "func-named-parameters": ["warn", 3],
    "func-order": "off",
    "func-param-name-mixedcase": "warn",
    "func-visibility": ["warn", { "ignoreConstructors": true }],
    "function-max-lines": ["warn", 30],
    "gas-calldata-parameters": "warn",
    "gas-custom-errors": "warn",
    "gas-increment-by-one": "warn",
    "gas-indexed-events": "off",
    "gas-length-in-loops": "warn",
    "gas-multitoken1155": "warn",
    "gas-named-return-values": "warn",
    "gas-small-strings": "warn",
    "gas-strict-inequalities": "warn",
    "gas-struct-packing": "warn",
    "imports-on-top": "warn",
    "imports-order": "warn",
    "immutable-vars-naming": "off",
    "interface-starts-with-i": "warn",
    "mark-callable-contracts": "off",
    "max-line-length": ["warn", 80],
    "max-states-count": ["warn", 10],
    "modifier-name-mixedcase": "warn",
    "multiple-sends": "warn",
    "named-parameters-mapping": "warn",
    "no-complex-fallback": "warn",
    "no-console": "warn",
    "no-empty-blocks": "warn",
    "no-global-import": "warn",
    "no-inline-assembly": "off",
    "no-unused-import": "warn",
    "no-unused-vars": "warn",
    "not-rely-on-block-hash": "warn",
    "not-rely-on-time": "off",
    "one-contract-per-file": "warn",
    "ordering": "off",
    "payable-fallback": "warn",
    "private-vars-leading-underscore": "off",
    "quotes": ["warn", "single"],
    "reason-string": "off",
    "reentrancy": "warn",
    "state-visibility": "off",
    "use-forbidden-name": "warn",
    "var-name-mixedcase": "warn",
    "visibility-modifier-order": "warn"
  }
}
